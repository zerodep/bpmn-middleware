{
  "name": "bpmn-middleware",
  "version": "0.0.4",
  "description": "BPMN engine express middleware",
  "type": "module",
  "main": "dist/main.cjs",
  "module": "src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha -R dot",
    "posttest": "npm run lint && npm run dist && npm run tsd",
    "lint": "eslint . --cache",
    "tsd": "tsd",
    "prepack": "npm run dist",
    "cov:html": "c8 -r html -r text mocha -R dot",
    "test:lcov": "c8 -r lcov mocha && npm run lint",
    "dist": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerodep/bpmn-middleware.git"
  },
  "keywords": [
    "bpmn",
    "middleware",
    "express"
  ],
  "author": {
    "name": "Zerodep AB",
    "url": "https://github.com/zerodep"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zerodep/bpmn-middleware/issues"
  },
  "homepage": "https://github.com/zerodep/bpmn-middleware#readme",
  "devDependencies": {
    "@bonniernews/httptest": "^2.0.0",
    "@onify/flow-extensions": "^5.0.2",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@types/express": "^4.17.17",
    "bpmn-engine": "^17.1.0",
    "c8": "^8.0.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "chai": "^4.3.7",
    "chronokinesis": "^5.0.2",
    "eslint": "^8.41.0",
    "eslint-config-exp": "^0.6.2",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "got": "^13.0.0",
    "mocha": "^10.2.0",
    "mocha-cakes-2": "^3.3.0",
    "nock": "^13.3.1",
    "rollup": "^3.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "supertest": "^6.3.3",
    "tsd": "^0.28.1"
  },
  "dependencies": {
    "lru-cache": "^10.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "peerDependencies": {
    "bpmn-engine": ">=15",
    "express": ">=4"
  },
  "files": [
    "src/",
    "dist/",
    "index.d.ts"
  ],
  "c8": {
    "include": ["src/"]
  }
}
